-			force_stop = false;
-
-		if (force_stop && diff_ms < 0)
-			vibrator_schedule_work(&vib->work_vibrator_off,
-				msecs_to_jiffies(vib->warmup_ms));
-	}
-	spin_unlock_irqrestore(&vib->spinlock, flags);
-}

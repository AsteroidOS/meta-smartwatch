From f90bd7c5af28220a5d9ca3180a594ca947640d30 Mon Sep 17 00:00:00 2001
From: Philip Russell <argosphil@murena.io>
Date: Sun, 3 Sep 2023 15:57:58 +0000
Subject: [PATCH] Makefile: don't make CC be a python wrapper

---
 Makefile | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/Makefile b/Makefile
index 7db1fbd7f663..efa985ad2014 100644
--- a/Makefile
+++ b/Makefile
@@ -362,7 +362,7 @@ CHECK		= sparse
 
 # Use the wrapper for the compiler.  This wrapper scans for new
 # warnings and causes the build to stop upon encountering them
-CC		= $(PYTHON) $(srctree)/scripts/gcc-wrapper.py $(REAL_CC)
+CC		= $(REAL_CC)
 
 CHECKFLAGS     := -D__linux__ -Dlinux -D__STDC__ -Dunix -D__unix__ \
 		  -Wbitwise -Wno-return-void $(CF)

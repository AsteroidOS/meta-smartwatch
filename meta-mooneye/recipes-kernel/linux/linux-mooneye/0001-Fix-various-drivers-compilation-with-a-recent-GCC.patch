+
 	if (uart->read_allow(uart))
 	    mtk_uart_dma_vfifo_rx_tasklet_str(arg);
-    }
+
     atomic_dec(&vfifo->entry);
     spin_unlock_irqrestore(&vfifo->iolock, flags);
 }
